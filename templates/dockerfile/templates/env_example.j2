# Environment variables for {{ app_name }}
# Copy this file to .env and update values

# Application
APP_NAME={{ app_name }}
APP_ENV=production
PORT={{ port }}
LOG_LEVEL=info

# Database (example)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME={{ app_name }}
# DB_USER={{ app_name }}
# DB_PASSWORD=changeme

# Redis (example)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=

# API Keys (example)
# API_KEY=your-api-key-here
# SECRET_KEY=your-secret-key-here

# Feature Flags
# FEATURE_X_ENABLED=true

# External Services
# SMTP_HOST=smtp.example.com
# SMTP_PORT=587
# SMTP_USER=
# SMTP_PASSWORD=
